generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model health_check {
  id         Int       @id @default(autoincrement())
  updated_at DateTime? @default(now()) @db.Timestamptz(6)
}
